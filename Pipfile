[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
bin = "env PYTHONPATH=src pipenv run python3"
lint = "python3 -m pylint --output-format=colorized src -v"
lintfix = "python3 -m autopep8 -r src -v -aa -i"

[dev-packages]
pylint = "*"
black = "*"
autopep8 = "*"

[packages]
protobuf = "*"
tqdm = "*"
transformers = "*"
torch = "*"
numpy = "*"

[requires]
python_version = "3.7"
